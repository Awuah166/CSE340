<% if (title) { %>
    <h1><%= title %></h1>
<% } %>

<%- messages() %>

<section class="inv-management">
    <p>Use the link below to manage classifications and inventory items</p>
    <ul class="inv-management-links">
        <li>
            <a href="/inv/add-classification" title="Add a new classification">Add New Classification</a>
        </li>

        <li>
            <a href="/inv/add-inventory" title="Add a new inventory item">Add New Inventory</a>
        </li>
    </ul>

    <h2>Manage Inventory</h2>
    <p>Choose a classification to see those vehicles</p>
    
    <%- classificationSelect %>

    <table id="inventoryDisplay"></table>
    <noscript>JavaScript must be enabled to use this page</noscript>

    <script src="../../js/inventory.js"></script>
</section>